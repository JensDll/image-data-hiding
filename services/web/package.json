{
  "private": true,
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "test": "vitest",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0 --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "fix": "pnpm run format && pnpm run lint"
  },
  "dependencies": {
    "@tailwindcss/forms": "0.5.0",
    "@tailwindcss/typography": "0.5.2",
    "@vitejs/plugin-vue": "2.3.1",
    "autoprefixer": "10.4.4",
    "esbuild": "0.14.36",
    "esbuild-register": "3.3.2",
    "gsap": "3.10.3",
    "postcss": "8.4.12",
    "tailwindcss": "3.0.24",
    "unplugin-auto-import": "0.7.1",
    "unplugin-icons": "0.14.1",
    "unplugin-vue-components": "0.19.3",
    "validierung": "1.0.1",
    "vite": "2.9.4",
    "vue": "3.2.33",
    "vue-router": "4.0.14",
    "vue-tsc": "0.34.6"
  },
  "devDependencies": {
    "@iconify-json/heroicons-outline": "1.1.1",
    "@iconify-json/heroicons-solid": "1.1.1",
    "@iconify-json/mdi": "1.1.9",
    "@types/node": "17.0.24",
    "@types/tailwindcss": "3.0.10",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-vue": "8.6.0",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.8",
    "typescript": "4.6.3",
    "vitest": "0.9.3"
  }
}
