{
  "private": true,
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "test": "vitest",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0 --fix --ignore-path .gitignore --ignore-pattern config",
    "format": "prettier --write .",
    "fix": "pnpm run format && pnpm run lint"
  },
  "dependencies": {
    "@iconify-json/heroicons-outline": "1.1.1",
    "@iconify-json/heroicons-solid": "1.1.1",
    "@iconify-json/mdi": "1.1.13",
    "@tailwindcss/forms": "0.5.1",
    "@tailwindcss/typography": "0.5.2",
    "@types/node": "17.0.34",
    "@types/tailwindcss": "3.0.10",
    "@typescript-eslint/eslint-plugin": "5.24.0",
    "@typescript-eslint/parser": "5.24.0",
    "@vitejs/plugin-vue": "2.3.3",
    "@vueuse/core": "8.5.0",
    "autoprefixer": "10.4.7",
    "esbuild": "0.14.39",
    "esbuild-register": "3.3.2",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-vue": "8.7.1",
    "gsap": "3.10.4",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.11",
    "tailwindcss": "3.0.24",
    "tailwindcss-plugin-icons": "1.0.5",
    "typescript": "4.6.4",
    "unplugin-auto-import": "0.7.1",
    "unplugin-vue-components": "0.19.5",
    "validierung": "1.0.2",
    "vite": "2.9.9",
    "vitest": "0.12.6",
    "vue": "3.2.33",
    "vue-router": "4.0.15",
    "vue-tsc": "0.34.15"
  }
}
