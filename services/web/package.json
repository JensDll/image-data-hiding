{
  "private": true,
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "test": "vitest",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0 --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "fix": "pnpm run format && pnpm run lint"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@vitejs/plugin-vue": "^2.2.2",
    "autoprefixer": "^10.4.2",
    "esbuild": "^0.14.23",
    "esbuild-register": "^3.3.2",
    "gsap": "^3.9.1",
    "pinia": "^2.0.11",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-icons": "^0.13.2",
    "unplugin-vue-components": "^0.17.21",
    "validierung": "^1.0.1",
    "vite": "^2.8.4",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.31.4"
  },
  "devDependencies": {
    "@iconify-json/heroicons-outline": "^1.1.1",
