{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0 --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "fix": "pnpm run format:fix && pnpm run lint:fix"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "autoprefixer": "^10.4.2",
    "esbuild": "^0.14.21",
    "esbuild-register": "^3.3.2",
    "gsap": "^3.9.1",
    "pinia": "^2.0.11",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.19",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-icons": "^0.13.1",
    "unplugin-vue-components": "^0.17.18",
    "validierung": "^1.0.1",
    "vite": "^2.8.0",
    "vue": "^3.2.30",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.31.2"
  },
  "devDependencies": {
    "@iconify-json/heroicons-solid": "^1.0.2",
    "@iconify-json/ic": "^1.0.8",
    "@iconify-json/mdi": "^1.0.17",
    "@types/node": "^17.0.17",
    "@types/tailwindcss": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.4.1",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "typescript": "^4.5.5"
  }
}
